FNUM=3
ELSE
IF SNUM=6
FNUM=4
ELSE
IF SNUM=4
FNUM=5
ELSE
IF SNUM=5
FNUM=6
ELSE
IF SNUM=7
FNUM=0
ENDIF
ENDIF
ENDIF
ENDIF
ENDIF
ENDIF
ENDIF
ENDIF
